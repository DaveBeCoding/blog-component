<!DOCTYPE html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Blog</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- <link href="https://fonts.googleapis.com/css2?family=Black+Ops+One&family=Monoton&family=Poller+One&display=swap" rel="stylesheet">    -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <link rel="stylesheet" href="">
    </head>
    <body>
        <!-- <h1 style="font-family: 'Black Ops One', cursive;" >THIS IS HTML</h1>   -->
        <div class="container">
            <h1 class="mb-4">Blog Articles</h1>
            <a href="/articles/new" class="btn btn-success">New Article</a>
            <% articles.forEach(article => { %>
                <div class="card mt-4">
                    <div class="card-body">
                        <h4 class="card-title"><%= article.title %></h4>
                        <div class="card-subtitle text-muted mb-2">
                            <%= article.createdAt.toLocaleDateString() %> 
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </body>
    <script src="" async defer></script>
</html>